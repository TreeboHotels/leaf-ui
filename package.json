{
  "name": "leaf-ui",
  "version": "0.0.0-development",
  "description": "üçÉ Leaf-UI: A react component library built using styled-components",
  "main": "lib/web.js",
  "module": "es/web.js",
  "scripts": {
    "start": "run-s storybook",
    "native": "react-native-scripts start",
    "storybook": "NODE_ENV=es start-storybook -p 9000 -c ./storybook",
    "storybook:build": "NODE_ENV=cjs build-storybook -o docs -c ./storybook",
    "storybook:native": "storybook start -p 7007 -c ./storybook/native",
    "semantic-release": "semantic-release",
    "build": "run-s build:clean && run-p build:es build:cjs",
    "build:clean": "rimraf es lib",
    "build:es": "NODE_ENV=es babel src -d es --ignore *.test.js --ignore *.story.js",
    "build:cjs": "NODE_ENV=cjs babel src -d lib --ignore *.test.js --ignore *.story.js",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.js'",
    "test": "jest"
  },
  "files": [
    "lib/**/*",
    "es/**/*",
    "native.js"
  ],
  "lint-staged": {
    "*.js": [
      "eslint",
      "stylelint"
    ]
  },
  "dependencies": {
    "downshift": "1.30.0",
    "expo": "23.0.4",
    "formik": "0.11.4",
    "react-overlays": "0.8.3",
    "yup": "0.24.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.3.15",
    "@storybook/addon-knobs": "3.3.15",
    "@storybook/addon-options": "3.3.15",
    "@storybook/react": "3.3.15",
    "@storybook/react-native": "3.3.15",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.2.2",
    "babel-jest": "22.4.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-syntax-trailing-function-commas": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-export-extensions": "6.22.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "4.18.2",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.9.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "husky": "0.14.3",
    "jest": "22.4.2",
    "lint-staged": "7.0.0",
    "npm-run-all": "4.1.2",
    "prop-types": "15.6.1",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-native": "0.50.3",
    "react-native-scripts": "1.8.1",
    "rimraf": "2.6.2",
    "semantic-release": "11.0.2",
    "styled-components": "3.2.1",
    "stylelint": "9.1.1",
    "stylelint-config-standard": "18.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.3.1"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-native": "^0.50.3",
    "styled-components": "^3.1.6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/treebohotels/leaf-ui.git"
  },
  "author": "Lakshya Ranganath <lakshya.ranganath@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/treebohotels/leaf-ui/issues"
  },
  "homepage": "https://github.com/treebohotels/leaf-ui#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
