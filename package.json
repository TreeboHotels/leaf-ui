{
  "name": "leaf-ui",
  "version": "0.2.2",
  "description": "üçÉ Leaf-UI: A react component library built using styled-components",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "start": "run-s storybook",
    "storybook": "BABEL_ENV=es start-storybook -p 9000",
    "storybook:build": "BABEL_ENV=cjs build-storybook -o docs",
    "clean": "rimraf es lib",
    "build": "run-s clean && run-p build:es build:cjs",
    "build:es": "BABEL_ENV=es babel src -d es --ignore *.test.js --ignore *.story.js",
    "build:cjs": "BABEL_ENV=cjs babel src -d lib --ignore *.test.js --ignore *.story.js",
    "prepublish": "run-p lint test build",
    "precommit": "lint-staged",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint .",
    "lint:stylelint": "stylelint '**/*.js'",
    "test": "BABEL_ENV=es jest"
  },
  "files": [
    "lib/**/*",
    "es/**/*"
  ],
  "lint-staged": {
    "*.js": [
      "eslint",
      "stylelint"
    ]
  },
  "devDependencies": {
    "@storybook/addon-actions": "3.2.15",
    "@storybook/addon-knobs": "3.2.15",
    "@storybook/addon-options": "3.2.15",
    "@storybook/react": "3.2.15",
    "babel-cli": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-jest": "21.2.0",
    "babel-preset-treebo": "github:treebohotels/babel-preset-treebo",
    "eslint": "4.10.0",
    "eslint-config-treebo": "github:treebohotels/eslint-config-treebo",
    "husky": "0.14.3",
    "jest": "21.2.1",
    "lint-staged": "5.0.0",
    "npm-run-all": "4.1.2",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "rimraf": "2.6.2",
    "styled-components": "2.2.3",
    "stylelint": "8.2.0",
    "stylelint-config-standard": "17.0.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-processor-styled-components": "1.0.0"
  },
  "peerDependencies": {
    "react": "^15.3.0 || ^16.0.0",
    "react-dom": "^15.3.0 || ^16.0.0",
    "styled-components": "^2.2.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/treebohotels/leaf-ui.git"
  },
  "author": "Lakshya Ranganath <lakshya.ranganath@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/treebohotels/leaf-ui/issues"
  },
  "homepage": "https://github.com/treebohotels/leaf-ui#readme"
}
